{
  "manifest_version": 3,
  "name": "Кинопоиск qx",
  "version": "1.0",
  "description": "Добавляет кнопку для перехода на Кинопоиск qx",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["*://www.kinopoisk.ru/film/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
} 